programa
{
	
	funcao inicio()
	{
		inteiro a, b
		escreva("Digite os números: ", "\n")
		leia(a,b)

		soma(a,b)
	
	}

	funcao soma(inteiro n1, inteiro n2){
		escreva("Soma: ", n1+n2)
	}
}
